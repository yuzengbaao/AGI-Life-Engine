# LLM Provider Configuration
LLM_PROVIDER_PRIORITY=dashscope,zhipu,deepseek
LLM_HTTP_TIMEOUT=60
LLM_IGNORE_PROXY=false

# Force use of remote API to avoid VRAM overflow
USE_LOCAL_LLM=false

# DeepSeek API
DEEPSEEK_API_KEY=your_deepseek_api_key_here
DEEPSEEK_MODEL=deepseek-chat
DEEPSEEK_TEMPERATURE=0.2
DEEPSEEK_MAX_TOKENS=2048

# DashScope (Alibaba Qwen)
DASHSCOPE_API_KEY=your_dashscope_api_key_here
DASHSCOPE_MODEL=qwen-plus
DASHSCOPE_TEMPERATURE=0.2
DASHSCOPE_MAX_TOKENS=2048

# Zhipu AI
ZHIPU_API_KEY=your_zhipu_api_key_here
ZHIPU_MODEL=glm-4-flash
ZHIPU_TEMPERATURE=0.2
ZHIPU_MAX_TOKENS=2048

# Gemini (Google AI)
GEMINI_API_KEY=your_gemini_api_key_here
GEMINI_MODEL=gemini-2.0-flash
GEMINI_TEMPERATURE=0.2
GEMINI_MAX_TOKENS=2048

# HuggingFace
HUGGINGFACE_API_KEY=your_huggingface_api_key_here
HUGGINGFACE_MODEL=Qwen/Qwen2.5-3B-Instruct
HUGGINGFACE_TEMPERATURE=0.2
HUGGINGFACE_MAX_NEW_TOKENS=2048

# World Labs 3D World Generation
WORLDLABS_API_KEY=your_worldlabs_api_key_here
WORLDBASE_URL=https://api.worldlabs.ai/v1
WORLDBASE_TIMEOUT=300

# Local LLM Configuration
LOCAL_LLM_MODEL=d:/TRAE_PROJECT/AGI/models/Qwen3-VL-2B-Instruct
LOCAL_LLM_QUANTIZATION=4bit
LOCAL_LLM_MAX_TOKENS=2048

# DeepSeek OCR Local Path
DEEPSEEK_OCR_LOCAL_DIR=d:/TRAE_PROJECT/AGI/models/DeepSeek-OCR

# Whisper Local Path
WHISPER_MODEL_LOCAL_DIR=d:/TRAE_PROJECT/AGI/models/whisper-base

# Embedding Model Local Path
EMBEDDING_MODEL_LOCAL_DIR=d:/TRAE_PROJECT/AGI/models/all-MiniLM-L6-v2

# GitHub
GITHUB_TOKEN=your_github_token_here

# Baidu OCR API
BAIDU_OCR_APP_ID=your_baidu_ocr_app_id_here
BAIDU_OCR_API_KEY=your_baidu_ocr_api_key_here
BAIDU_OCR_SECRET_KEY=your_baidu_ocr_secret_key_here
BAIDU_OCR_AES_KEY=your_baidu_ocr_aes_key_here

# HuggingFace Mirror (for faster downloads in China)
HF_ENDPOINT=https://hf-mirror.com

# OpenRouter
OPENROUTER_TOKEN=your_openrouter_token_here

# QWeather (Weather API)
QWEATHER_API_KEY=your_qweather_api_key_here

# OpenWeather (Weather API)
OPENWEATHER_API_KEY=your_openweather_api_key_here

# MiniMax
MINIMAX_API_KEY=your_minimax_api_key_here
MINIMAX_GROUP_ID=AGI
MINIMAX_MODEL=abab6.5-chat
MINIMAX_TEMPERATURE=0.2
MINIMAX_MAX_TOKENS=2048

# Anthropic Claude
ANTHROPIC_API_KEY=your_anthropic_api_key_here
ANTHROPIC_MODEL=us.anthropic.claude-sonnet-4-5-20250929-v1:0
ANTHROPIC_TEMPERATURE=0.2

# LLM First Mode Configuration
LLM_FIRST_MODE=true
DETERMINISTIC_MODE=false
TOOL_FIRST_MODE=true
HALLUCINATION_CONTROL=moderate
HALLUCINATION_BLOCKER_ENABLED=true
MIN_DECISION_CONFIDENCE=0.70
